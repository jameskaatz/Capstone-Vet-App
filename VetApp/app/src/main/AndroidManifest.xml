<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vetapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConversationActivity"></activity>
        <activity android:name=".NewConversationActivity" />
        <activity android:name=".UserSearchActivity" />
        <activity android:name=".AddContactActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewAnimals"
            android:label="@string/title_activity_view_animals"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".VetView" />
        <activity android:name=".FarmView" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>